{
	"$schema": "../node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"cyph.ws": {
			"root": "",
			"sourceRoot": "src",
			"projectType": "application",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist",
						"index": "src/index.html",
						"main": "src/js/cyph.ws/main.ts",
						"tsConfig": "src/tsconfig.app.json",
						"polyfills": "src/js/cyph.ws/polyfills.ts",
						"assets": [
							"src/assets",
							"src/favicon.ico",
							"src/manifest.json",
							"src/serviceworker.js"
						],
						"styles": [
							"src/css/cyph.ws.scss"
						],
						"scripts": []
					},
					"configurations": {
						"backup": {
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/environment.backup.ts"
								}
							]
						},
						"beta": {
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/environment.beta.ts"
								}
							]
						},
						"dev": {
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/environment.dev.ts"
								}
							]
						},
						"e2e": {
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/environment.e2e.ts"
								}
							]
						},
						"local": {
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/environment.local.ts"
								}
							]
						},
						"localBackup": {
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/environment.local-backup.ts"
								}
							]
						},
						"localBeta": {
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/environment.local-beta.ts"
								}
							]
						},
						"localDev": {
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/environment.local-dev.ts"
								}
							]
						},
						"localMaster": {
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/environment.local-master.ts"
								}
							]
						},
						"localStaging": {
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/environment.local-staging.ts"
								}
							]
						},
						"master": {
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/environment.master.ts"
								}
							]
						},
						"production": {
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/environment.prod.ts"
								}
							]
						},
						"staging": {
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/environment.staging.ts"
								}
							]
						},
						"tmp": {
							"fileReplacements": [
								{
									"replace": "src/js/environments/environment.ts",
									"with": "src/js/environments/.environment.tmp.ts"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "cyph.ws:build"
					},
					"configurations": {
						"backup": {
							"browserTarget": "cyph.ws:build:backup"
						},
						"beta": {
							"browserTarget": "cyph.ws:build:beta"
						},
						"dev": {
							"browserTarget": "cyph.ws:build:dev"
						},
						"e2e": {
							"browserTarget": "cyph.ws:build:e2e"
						},
						"local": {
							"browserTarget": "cyph.ws:build:local"
						},
						"localBackup": {
							"browserTarget": "cyph.ws:build:localBackup"
						},
						"localBeta": {
							"browserTarget": "cyph.ws:build:localBeta"
						},
						"localDev": {
							"browserTarget": "cyph.ws:build:localDev"
						},
						"localMaster": {
							"browserTarget": "cyph.ws:build:localMaster"
						},
						"localStaging": {
							"browserTarget": "cyph.ws:build:localStaging"
						},
						"master": {
							"browserTarget": "cyph.ws:build:master"
						},
						"production": {
							"browserTarget": "cyph.ws:build:production"
						},
						"staging": {
							"browserTarget": "cyph.ws:build:staging"
						},
						"tmp": {
							"browserTarget": "cyph.ws:build:tmp"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "cyph.ws:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"karmaConfig": "./karma.conf.js",
						"polyfills": "src/js/cyph.ws/polyfills.ts",
						"tsConfig": "src/tsconfig.spec.json",
						"scripts": [],
						"styles": [
							"src/css/cyph.ws.scss"
						],
						"assets": [
							"src/assets",
							"src/favicon.ico",
							"src/manifest.json",
							"src/serviceworker.js"
						]
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"src/tsconfig.app.json",
							"src/tsconfig.spec.json"
						],
						"exclude": []
					}
				}
			}
		},
		"cyph.ws-e2e": {
			"root": "",
			"sourceRoot": "",
			"projectType": "application",
			"cli": {},
			"schematics": {},
			"architect": {
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "./protractor.conf.js",
						"devServerTarget": "cyph.ws:serve"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"e2e/tsconfig.e2e.json"
						],
						"exclude": []
					}
				}
			}
		}
	},
	"cli": {},
	"schematics": {
		"@schematics/angular:component": {
			"prefix": "cyph",
			"styleext": "scss"
		},
		"@schematics/angular:directive": {
			"prefix": "cyph"
		}
	}
}
