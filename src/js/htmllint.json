{
	"attr-bans": [
		"background",
		"bgcolor",
		"border",
		"frameborder",
		"longdesc",
		"marginwidth",
		"marginheight",
		"scrolling",
		"style"
	],
	"attr-name-ignore-regex": "^(\\*|\\[).*",
	"attr-name-style": false,
	"attr-no-dup": true,
	"attr-no-unsafe-char": false,
	"attr-quote-style": "single",
	"attr-req-value": false,
	"class-no-dup": false,
	"class-style": false,
	"href-style": false,
	"id-class-no-ad": true,
	"id-class-style": false,
	"id-no-dup": false,
	"img-req-alt": true,
	"img-req-src": false,
	"indent-style": "tabs",
	"label-req-for": true,
	"line-max-len": 100,
	"spec-char-escape": false,
	"tag-bans": ["script", "style"],
	"tag-close": true,
	"tag-name-lowercase": true,
	"tag-name-match": true,
	"tag-self-close": "always"
}
